@using Beyova;
@using Beyova.ApiTracking;
@using Beyova.Gravity;
@model List<ProductInfo>

<div class="table-responsive">
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Token</th>
                <th>Created Stamp</th>
                <th>Last Updated Stamp</th>
                <th>Management</th>
            </tr>
        </thead>
        <tbody>
            @{
                if (Model.HasItem())
                {
                    foreach (var one in Model)
                    {
                        <tr>
                            <td><a href="javascript:showEntity('@(one.Key)')">@(one.Name)</a></td>
                            <td>@(one.Token)</td>
                            <td>@(one.CreatedStamp.ToFullDateTimeString())</td>
                            <td>@(one.LastUpdatedStamp.ToFullDateTimeString())</td>
                            <td>
                                <a href="@(Url.CreateAdminUserPanelUrl(one.Key))" title="Users" target="_blank"><i class="fa fa-users"></i></a>
                                <a href="@(Url.CreateAdminRolePanelUrl(one.Key))" title="Roles" target="_blank"><i class="fa fa-user-circle"></i></a>
                                <a href="@(Url.CreateAdminPermissionPanelUrl(one.Key))" title="Roles" target="_blank"><i class="fa fa-cubes"></i></a>

                                <a href="@(Url.Action("AuthenticationIntegrationCodeSample", new { productKey=one.Key}))" target="_blank" title="Integration Code"><i class="fa fa-code"></i></a>
                            </td>
                        </tr>
                    }
                }
                else
                {
                    <text>
                        <tr><td colspan="5">No Result.</td></tr>

                    </text>
                }
            }

        </tbody>
    </table>
</div>